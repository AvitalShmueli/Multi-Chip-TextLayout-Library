<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MultiChipTextLayout">
        <attr name="textSize" format="dimension" />
        <attr name="ems" format="integer" />
        <attr name="strokeColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="hintText" format="string"/>
        <attr name="startIconDrawable" format="reference"/>
        <attr name="chipIconDrawable" format="reference"/>
        <attr name="startIconTint" format="color"/>
        <attr name="chipIconTint" format="color"/>
        <attr name="chipIconStartPadding" format="dimension"/>
        <attr name="chipIconSize" format="dimension"/>
        <attr name="chipCheckable" format="boolean"/>
        <attr name="chipClickable" format="boolean"/>
        <attr name="chipCloseEnabled" format="boolean"/>
        <attr name="chipTextSize" format="dimension"/>
        <attr name="chipTextStartPadding" format="dimension"/>
        <attr name="chipTextEndPadding" format="dimension"/>
        <attr name="chipPaddingLeft" format="dimension"/>
        <attr name="chipPaddingRight" format="dimension"/>
        <attr name="chipPaddingTop" format="dimension"/>
        <attr name="chipPaddingBottom" format="dimension"/>
        <attr name="chipStrokeColor" format="color"/>
        <attr name="chipStrokeWidth" format="dimension"/>
        <attr name="chipBackgroundColor" format="color"/>
        <attr name="chipTextColor" format="color"/>
        <attr name="chipCloseIconTint" format="color"/>
        <attr name="enableAddValuesManually" format="boolean"/>
        <attr name="enableMultipleSelection" format="boolean"/>
        <attr name="chipsLayoutMode" format="enum">
            <enum name="BELOW" value="0"/>
            <enum name="INLINE" value="1"/>
            <enum name="COMBO_BOX" value="2"/>
        </attr>
    </declare-styleable>

</resources>